<Window x:Class="iS3.Core.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:iS3.Core.Test"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <WrapPanel>
        <StackPanel>
            <Label Content="Borehole"/>
            <Label Content="ID" Margin="5"/>
            <TextBox Width="100" Name="a" Margin="5"/>
            <Label Content="Name" Margin="5"/>
            <TextBox Width="100" Name="b" Margin="5"/>
            <Label Content="Top" Margin="5"/>
            <TextBox Width="100" Name="c" Margin="5"/>
        </StackPanel>
        <StackPanel>
            <Button Name="view" Content="view" Click="view_Click" Margin="5"/>
            <Button Name="add" Content="add" Click="add_Click" Margin="5"/>
            <Button Name="modify" Content="modify" Click="modify_Click" Margin="5"/>
            <Button Name="detele" Content="detele" Click="detele_Click" Margin="5"/>

        </StackPanel>
        <TextBox Name="output" Width="300" Height="300" />
    </WrapPanel>
</Window>
